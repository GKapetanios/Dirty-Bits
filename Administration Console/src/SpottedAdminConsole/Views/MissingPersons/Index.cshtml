@model IEnumerable<SpottedAdminConsole.Models.MissingPerson>

@{
    ViewBag.Title = "Missing Persons";
}

<h2>Missing Persons</h2>


<a asp-controller="MissingPersons" asp-action="Create">Create New</a>


<table class="table">
    <tr>
        <th>Id</th>
        <th>Name</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
        </tr>
    }
</table>